{
  "name": "portfolio",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "homepage": "https://liuddy.github.io/portfolio",
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not and_qq > 0",
    "not and_uc > 0",
    "not kaios > 0",
    "not op_mini all"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && copy dist\\index.html dist\\404.html",
    "preview": "vite build && vite preview",
    "deploy": "gh-pages -d dist",
    "format": "prettier --write src/",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "linthtml": "htmlhint *.html",
    "lintcss": "stylelint src/**/*.{css,scss,vue} --fix"
  },
  "dependencies": {
    "pinia": "^2.1.7",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "browserslist": "^4.25.0",
    "doiuse": "^6.0.5",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "gh-pages": "^6.3.0",
    "htmlhint": "^1.3.0",
    "postcss-html": "^1.8.0",
    "prettier": "^3.2.5",
    "stylelint": "^16.20.0",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-no-unsupported-browser-features": "^8.0.4",
    "vite": "^5.2.8"
  }
}
